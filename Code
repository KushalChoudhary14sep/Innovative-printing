let length = Int(readLine()!)
let breadth = Int(readLine()!)


var arr = [Int]()

for _ in 1...length! {
    if let input = Int(readLine()!){
        arr.append(input)
    }
}

for i in 0..<length! {
    print("x",terminator:"")
    var temp = 0
    
    for _ in 2...breadth! {
        if(temp == arr[i]){
            print("x",terminator:"")
            temp = 0
            }
            else{
            print("o",terminator:"")
            temp += 1
        }
    }
print(" ")
    
}
